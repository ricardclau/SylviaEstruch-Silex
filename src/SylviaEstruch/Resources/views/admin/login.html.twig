{% extends 'adminlayout.html.twig' %}

{% block content %}
<div id="form_login">
    <form method="post" action="/admin/login">
    <table>
        <tr>
            <td>Login:</td>
            <td><input type="text" name="login" /></td>
        </tr>
        <tr>
            <td>Password:</td>
            <td><input type="password" name="password" /></td>
        </tr>
        <tr>
            <td colspan="2" align="center"><input type="submit" name="Login" /></td>
        </tr>
    </table>
    </form>
</div>
<br />
{% endblock %}